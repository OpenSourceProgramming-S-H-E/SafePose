     continue\n","\n","    keypoint_rows = []\n","\n","    for frame_file in tqdm(sorted(os.listdir(folder_path))):\n","        if not frame_file.endswith('.jpg'):\n","            continue\n","        frame_path = os.path.join(folder_path, frame_file)\n","\n","        keypoints = extract_keypoints(frame_path)\n","\n","        row = {'frame': frame_file}\n","        for i, (y, x, conf) in enumerate(keypoints):\n","            row[f'keypoint_{i}_x'] = float(x)\n","            row[f'keypoint_{i}_y'] = float(y)\n","            row[f'keypoint_{i}_conf'] = float(conf)\n","\n","        keypoint_rows.append(row)\n","\n","    # ✅ DataFrame으로 변환 후 CSV 저장\n","    df = pd.DataFrame(keypoint_rows)\n","    csv_path = os.path.join(csv_output_root, f'{folder}.csv')\n","    df.to_csv(csv_path, index=False)\n","    print(f'✅ {folder} → CSV saved to {csv_path}')"],"metadata":{"id":"trm9jwfbQk_t","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1750060284457,"user_tz":-540,"elapsed":3281606,"user":{"displayName":"임효주","userId":"05835219722875978959"}},"outputId":"45a9eb02-9438-47fe-80db-f332f4339070"},"execution_count":3,"outputs":[{"output_type":"stream","name":"stderr","text":["100%|██████████| 600/600 [01:33<00:00,  6.39it/s]\n"]},{"output_type":"stream","name":"stdout","text":["✅ 00002_H_A_N_C6 → CSV saved to /content/drive/MyDrive/Colab Notebooks/OpenVideo/CSV files/Suspicious/00002_H_A_N_C6.csv\n"]},{"output_type":"stream","name":"stderr","text":["100%|██████████| 600/600 [01:33<00:00,  6.44it/s]\n"]},{"output_type":"stream","name":"stdout","text":["✅ 00002_H_A_N_C8 → CSV saved to /content/drive/MyDrive/Colab Notebooks/OpenVideo/CSV files/Suspicious/00002_H_A_N_C8.csv\n"]},{"output_type":"stream","name":"stderr","text":["100%|██████████| 600/600 [01:30<00:00,  6.64it/s]\n"]},{"output_type":"stream","name":"stdout","text":["✅ 00002_H_A_N_C5 → CSV saved to /content/drive/MyDrive/Colab Notebooks/OpenVideo/CSV files/Suspicious/00002_H_A_N_C5.c